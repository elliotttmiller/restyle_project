# Environment Variables Template
# Copy this file to .env and add your real credentials

# Google Cloud
GOOGLE_APPLICATION_CREDENTIALS=path/to/your/google-credentials.json
GOOGLE_CLOUD_PROJECT=your-project-id
GOOGLE_CLOUD_LOCATION=us-central1

# AWS
AWS_ACCESS_KEY_ID=your-aws-access-key
AWS_SECRET_ACCESS_KEY=your-aws-secret-key
AWS_REGION_NAME=us-east-1

# eBay
EBAY_PRODUCTION_APP_ID=your-ebay-app-id
EBAY_PRODUCTION_CERT_ID=your-ebay-cert-id
EBAY_PRODUCTION_CLIENT_SECRET=your-ebay-client-secret
EBAY_PRODUCTION_REFRESH_TOKEN=your-ebay-refresh-token
EBAY_PRODUCTION_USER_TOKEN=your-ebay-user-token

# Django
SECRET_KEY=your-django-secret-key
DEBUG=True
ALLOWED_HOSTS=localhost,127.0.0.1

# Database
DATABASE_URL=sqlite:///db.sqlite3 